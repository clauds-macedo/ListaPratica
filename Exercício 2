#include <stdio.h>
#include<time.h>

int main(){
void primo (int i);
time_t t,tf,tempo, i = 0;

t= time(NULL);
do{ 

primo (i);
tf= time(NULL);
tempo = difftime(tf,t);
i++;
}while (  tempo<=60);









}

